{
  "name": "hatchery-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/cli",
    "packages/engine",
    "packages/frontend"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "npm run dev --workspace=packages/frontend",
    "lint": "eslint packages --ext .ts,.tsx",
    "typecheck": "npm run typecheck --workspace=packages/cli && npm run typecheck --workspace=packages/engine && npm run typecheck --workspace=packages/frontend",
    "test": "vitest run",
    "check": "npm run lint && npm run typecheck && npm run test && npm run build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@types/node": "^22.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.5.4",
    "typescript": "^5.7.0",
    "vitest": "^2.1.8"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
